    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/runtime": "^7.12.5",
        "@react-native-community/eslint-config": "^2.0.0",
        "@types/jest": "^26.0.16",
        "@types/react-native": "^0.63.37",
        "@types/react-redux": "^7.1.11",
