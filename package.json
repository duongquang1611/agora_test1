{
    "name": "sellme",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "init-project": "react-native eject && npm run pod-install",
        "start": "react-native start",
        "android-dev": "react-native run-android --no-jetifier --variant=devDebug --appId 'jp.sellme.dev'",
        "android-staging": "cd android && ./gradlew clean && cd .. && react-native run-android --no-jetifier --variant=stagingDebug --appId 'jp.sellme.staging'",
        "android-product": "cd android && ./gradlew clean && cd .. && react-native run-android --no-jetifier --variant=productDebug --appId 'jp.apps.sellme'",
        "android-apk-staging": "export ENVFILE=.env.staging && cd android && ./gradlew assembleStagingRelease && cd ..",
        "android-apk-dev": "export ENVFILE=.env.development && cd android && ./gradlew assembleDevRelease && cd ..",
        "android-apk-product": "export ENVFILE=.env.production && cd android && ./gradlew assembleProductRelease && cd ..",
        "android-bundle-product": "export ENVFILE=.env.production && cd android && ./gradlew bundleProductRelease && cd ..",
        "android-bundle-staging": "export ENVFILE=.env.staging && cd android && ./gradlew bundleStagingRelease && cd ..",
        "ios-codepush-product": "appcenter codepush release-react -a Amela/sellme-ios -d Production -t 1.0.1",
        "ios-codepush-dev": "appcenter codepush release-react -a Amela/sellme-ios -d Development -t 1.0.1",
        "ios-codepush-staging": "appcenter codepush release-react -a Amela/sellme-ios -d Staging -t 1.0.1",
        "android-codepush-staging": "appcenter codepush release-react -a Amela/sellme-android -d Staging -t 1.0.0",
        "android-codepush-production": "appcenter codepush release-react -a Amela/sellme-android -d Production -t 1.0.0",
        "android-codepush-dev": "appcenter codepush release-react -a Amela/sellme-android -d Development -t 1.0.0",
        "ios": "react-native run-ios",
        "test": "jest",
        "pod-install": "cd ios && pod install && cd ..",
        "postinstall": "cd scripts && sh ./fix-lib.sh && cd .. && cd ios && pod install && cd .. && npx jetifier",
        "assets-link": "npx react-native link",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint:ts": "tslint 'src/**/*.{ts,tsx,js}'",
        "lint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
        "devtools": "react-devtools",
        "open-folder-apk": "open ./android/app/build/outputs/apk",
        "open-folder-aab": "open ./android/app/build/outputs/bundle",
        "rebase": "git checkout develop && git pull origin develop && git checkout - && git rebase -",
        "no-edit": "git add . && git commit --amend --no-edit && git push origin HEAD -f",
        "none": "git add . && git commit --amend --no-edit --no-verify && git push origin HEAD -f",
        "commit": "git add . && git commit -m 'Update Feature' && git push origin HEAD"
    },
    "dependencies": {
        "@react-native-community/async-storage": "^1.12.0",
